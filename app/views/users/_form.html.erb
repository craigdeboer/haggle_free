<%= form_for @user do |f| %>
	<%= f.label :first_name %>
	<%= f.text_field :first_name %>
	<%= f.label :last_name %>
	<%= f.text_field :last_name %>
	<%= f.label :user_name %>
	<%= f.text_field :user_name %>
	<%= f.label :email %>
	<%= f.text_field :email %>
	<%= f.label :admin %>
	<%= f.check_box :email %>
	<%= f.submit %>
<% end %>