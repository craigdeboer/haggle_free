<!-- When using render @category it iterates automatically -->
<table id="category-table">
	<tr>
		<td><%= category.name %></td>
		<td><%= link_to category, remote: true, method: :delete, data: { confirm: "Are you sure?" } do %>
			<button class="delete-button">Delete</button>
		<% end %></td>
	</tr>
</table>
